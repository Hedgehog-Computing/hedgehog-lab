/** 
*
* @author Jason Reynolds
* @param 
* @returns 
* 
* Function that's a unit test for lusolve.hhs. Tests if it gives the correct answer and if it accepts both array and Mat object
*/



function lusolve_test() {

    *import math: lusolve

    let a = new Mat([[2, 1], [3, 4]]);
    let b = [[3], [3]];

    if (!(lusolve(a, b) === [[1.8], [-0.6]])) {
        throw new Error('Unit test failed for lusolve');
    }
}